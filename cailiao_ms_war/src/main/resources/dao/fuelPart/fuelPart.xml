<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whut.cailiao.war.dao.fuelPart.FuelPartDao">
	<sql id="base_query">
		id,fuelItemList
	</sql>
	<!-- select -->
	<select id="findFuelPart" parameterType="java.lang.Integer"
		resultType="FuelPart">
		select
		<include refid="base_query" />
		from ms_tb_fuelPart
		where id=#{id}
	</select>

	<!-- delete -->
	<delete id="deleteFuelPart" parameterType="java.lang.Integer">
		delete from
		ms_tb_fuelPart
		where
		id=#{id}
	</delete>

	<!-- update -->
	<update id="updateFuelPart" parameterType="java.util.Map">
		update ms_tb_fuelPart
		<set>
			<if test="fuelItemList!=null">
				fuelItemList=#{fuelItemList}
			</if>
		</set>
	</update>

	<!-- insert -->
	<insert id="saveFuelPart" parameterType="java.util.Map">
		insert into
		ms_tb_fuelPart(fuelItemList)
		values(#{fuelItemList})
	</insert>
</mapper>