<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whut.cailiao.war.dao.questionnaire.ClinkerPartDao">

	<select id="findClinkerPart" parameterType="java.lang.Integer"
		resultType="ClinkerPart">
		select id,
		clinkerItemList
		from ms_tb_ClinkerPart
		where
		id=#{id,jdbcType=INTEGER}
	</select>

	<insert id="saveClinkerPart" parameterType="java.util.Map">
		insert into
		ms_tb_ClinkerPart(id,clinkerItemList)
		values (#{id},
		#{clinkerItemList})
	</insert>

	<delete id="deleteClinkerPart" parameterType="java.lang.Integer">
		delete from
		ms_tb_ClinkerPart
		where id=#{id}
	</delete>
	
	<update id="updateClinkerPart" parameterType="java.util.Map">
		update ms_tb_ClinkerPart
		<set>
			<if test="clinkerItemList!=null">
				clinkerItemList=#{clinkerItemList}
			</if>
		
		</set>
		where id=#{id}
	</update>

</mapper>