<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whut.cailiao.impl.dao.user.UserDao">

    <select id="getUserByAccount" parameterType="java.lang.String" resultType="User">
        SELECT
            accountId, password, factoryId, email, status
        FROM tb_user
        WHERE accountId = #{accountId, jdbcType=VARCHAR}
    </select>

</mapper>