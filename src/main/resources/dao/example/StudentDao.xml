<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.whut.cailiao.dao.example.StudentDao">
    <select id="selectBeanByName" resultType="student">
        SELECT * FROM ms_tb_student WHERE name = #{name}
    </select>

    <insert id="insertNewItem" parameterType="java.util.Map">
        INSERT INTO ms_tb_student (name, age) VALUES (#{name}, #{age})
    </insert>
</mapper>